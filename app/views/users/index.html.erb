<h1>Users</h1>
<table>
  <thead>
    <tr>
      <th>Name</th>
    </tr>
  </thead>
  <tbody>
  <% @users.each do |user| %>
    <tr>
      <td><%= link_to user.first_name.to_s + " " + user.last_name.to_s, user %></td>
      <td><%= link_to "Edit", edit_user_path(user) %></td>
      <td><%= link_to "Destroy", user, method: :delete %></td>
    </tr>
  <% end %>
  </tbody>
</table>

<p><%= link_to "Create User", new_user_path %></p>
